import java.util.*;
public class Pat01_Task04{
  public static void main(String args[]){
    Scanner sc = new Scanner(System.in);
    int a[] = new int[10];
    for(int i=0; i<10; i++){
      System.out.println("Enter a number");
      a[i] = sc.nextInt();
      if(i==0){
      }
      else{
        for(int j=i-1; j<10; j++){
          if(a[i]==a[j]){
            System.out.println("Enter another number");
            j--;
          }
        }
      }
    }
  }
}